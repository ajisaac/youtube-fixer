{
	"manifest_version": 3,
	"name": "youtube fixer",
	"version": "1.0",
	"description": "Fixes youtubes search issues.",
	"icons": {
		"16": "16x16.png",
		"32": "32x32.png",
		"48": "48x48.png",
		"128": "128x128.png",
		"144": "144x144.png"
	},
	"content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
				"https://www.youtube.com/results*"
      ]
    }
  ]
}
